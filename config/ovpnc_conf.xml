<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="ovpn.xsl" type="text/xsl"?>
<Nodes>
  <Node id="1">
	<Config-File>/tmp/test.conf</Config-File>
	<Directives>
		<Group id="1">
	 	    <Directive>
				<Name>local</Name>
				<Params>
					<VPN-Server>192.168.1.250</VPN-Server>
				</Params>
		    </Directive>
	 	    <Directive>
				<Name>port</Name>
				<Params>
			 		<Server-Port>8111</Server-Port>
			 	</Params>
		    </Directive>
	 	    <Directive>
				<Name>dev</Name>
				<Params>
			 		<Device></Device>
			 	</Params>
		    </Directive>
		    <Directive>
				<Name>proto</Name>
				<Params>
					<Protocol></Protocol>
				</Params>
	   		</Directive>	
		</Group>
		<Group id="2">
			<Directive>
				<Name>server</Name>
				<Params>
				    <Virtual-IP>10.0.10.0</Virtual-IP>
				    <Virtual-Netmask>255.255.255.0</Virtual-Netmask>
				</Params>
		   </Directive>
			<Directive>
				<Name>management</Name>
				<Params>
				    <Management-IP>127.0.0.1</Management-IP>
				    <Management-Port>7505</Management-Port>
				</Params>
		   </Directive>
		</Group>
		<Group id="3">
		    <Directive>
	 			<Name>user</Name>
				<Params>
					<User-Name>openvpn</User-Name>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>group</Name>
				<Params>
					<Group-Name>openvpn</Group-Name>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>verb</Name>
				<Params>
					<Verb>4</Verb>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>mute</Name>
				<Params>
					<Mute>5</Mute>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>status</Name>
				<Params>
					<Status-File>/var/log/status_vpn.log</Status-File>
					<Status-Sec>3</Status-Sec>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>comp-lzo</Name>
				<Params>
					<Comp-Lzo></Comp-Lzo>
				</Params>
		    </Directive>
		</Group>
		<Group id="4">
		    <Directive>
				<Name>client-to-client</Name>
		    </Directive>
		    <Directive>
				<Name>duplicate-cn</Name>
		    </Directive>
		    <Directive>
	 			<Name>max-clients</Name>
				<Params>
					<Max-Clients>64</Max-Clients>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>connect-freq</Name>
				<Params>
					<Connect-Freq-New>5</Connect-Freq-New>
					<Connect-Freq-Sec>2</Connect-Freq-Sec>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>keepalive</Name>
				<Params>
					<KeepAlive-Poll>10</KeepAlive-Poll>
					<KeepAlive-Dead>30</KeepAlive-Dead>
				</Params>
		    </Directive>	
		</Group>
		<Group id="5">
		    <Directive>
	 			<Name>cipher</Name>
				<Params>
					<Cipher></Cipher>
				</Params>
		    </Directive>	
		    <Directive>
	 			<Name>tls-auth</Name>
				<Params>
					<Tls-Key>/var/www/vpn/2.0/keys/ta.key</Tls-Key>
					<Tls-Mode>0</Tls-Mode>
				</Params>
		    </Directive>
			<Directive>
                <Name>crl-verify</Name>
			</Directive>
		    <Directive>
	 			<Name>ca</Name>
				<Params>
					<Ca>/some/path/serv.ca</Ca>
				</Params>
		    </Directive>	
		    <Directive>
	 			<Name>cert</Name>
				<Params>
					<Certificate>/some/path/serv.crt</Certificate>
				</Params>
		    </Directive>
		    <Directive>
	 			<Name>key</Name>
				<Params>
					<Key>/some/path/serv.key</Key>
				</Params>
		    </Directive>	
		    <Directive>
	 			<Name>dh</Name>
				<Params>
					<Dh>/some/path/dh1024.pem</Dh>
				</Params>
		    </Directive>	
		    <Directive>
	 			<Name>persist-key</Name>
		    </Directive>	
		    <Directive>
	 			<Name>persist-tun</Name>
		    </Directive>	
		</Group>
		<Group id="6">
			<Directive>
				<Name>push</Name>
				<Params>
	   				<Push-String>route 10.10.10.0 255.255.255.0</Push-String>
				</Params>
			    <Quoted>1</Quoted>
		   </Directive>
		    <Directive>
				<Name>push</Name>
				<Params>
	   				<Push-String>route-gateway dhcp</Push-String>
				</Params>
			    <Quoted>1</Quoted>
		   </Directive>
		    <Directive>
				<Name>push</Name>
				<Params>
	   				<Push-String>dhcp-option DNS 208.67.222.222</Push-String>
				</Params>
			    <Quoted>1</Quoted>
		   </Directive>
		</Group>
	</Directives>
	<Name>some node</Name>
  </Node>
</Nodes>
